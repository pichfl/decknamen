{
  "name": "game",
  "version": "1.0.0",
  "private": true,
  "description": "Small description for game goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "format": "npm-run-all --aggregate-output --continue-on-error --parallel format:*",
    "format:hbs": "prettier --parser glimmer **/*.hbs --write",
    "format:css": "npm-run-all --aggregate-output --continue-on-error --parallel format:css:*",
    "format:css:prettier": "prettier --write '**/*.css'",
    "format:yaml": "prettier --write '**/*.{yml,yaml}'",
    "format:js": "npm-run-all --aggregate-output --continue-on-error --parallel format:js:*",
    "format:js:prettier": "prettier --write '**/*.js'",
    "format:js:eslint": "eslint --fix .",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "start:local": "ember serve --environment=local",
    "ember": "ember"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@fortawesome/ember-fontawesome": "https://github.com/FortAwesome/ember-fontawesome#0.2.1",
